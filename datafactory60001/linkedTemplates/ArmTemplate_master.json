{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory60001"
		},
		"AzureSql_mydb_Database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSql_mydb_Database'"
		},
		"mystorage60001_linked_ss_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mystorage60001_linked_ss'"
		},
		"AzureSql_mydb_Database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "arise3.database.windows.net"
		},
		"AzureSql_mydb_Database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mydb"
		},
		"AzureSql_mydb_Database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin_arise"
		},
		"mystorage60001_linked_ss_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mystorage60001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory60001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSql_mydb_Database_password": {
						"value": "[parameters('AzureSql_mydb_Database_password')]"
					},
					"mystorage60001_linked_ss_accountKey": {
						"value": "[parameters('mystorage60001_linked_ss_accountKey')]"
					},
					"AzureSql_mydb_Database_properties_typeProperties_server": {
						"value": "[parameters('AzureSql_mydb_Database_properties_typeProperties_server')]"
					},
					"AzureSql_mydb_Database_properties_typeProperties_database": {
						"value": "[parameters('AzureSql_mydb_Database_properties_typeProperties_database')]"
					},
					"AzureSql_mydb_Database_properties_typeProperties_userName": {
						"value": "[parameters('AzureSql_mydb_Database_properties_typeProperties_userName')]"
					},
					"mystorage60001_linked_ss_properties_typeProperties_url": {
						"value": "[parameters('mystorage60001_linked_ss_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}